{
  "name": "aws-secrets-manager-read-action",
  "version": "0.0.1",
  "private": true,
  "description": "Github Action to read a secret value from AWS Secret Manager",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "dist": "npm run build && npm run package",
    "all": "npm run build && npm run format && npm run lint && npm run package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/t-botz/aws-secrets-manager-read-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "aws",
    "secrets"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "2.0.2",
    "@aws-sdk/client-secrets-manager": "3.966.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^20.19.28",
    "@typescript-eslint/parser": "^8.49.0",
    "@vercel/ncc": "^0.38.4",
    "eslint": "^9.39.2",
    "js-yaml": "^4.1.1",
    "prettier": "^3.8.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.52.0"
  }
}
