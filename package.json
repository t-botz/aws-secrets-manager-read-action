{
  "name": "aws-secrets-manager-read-action",
  "version": "0.0.1",
  "private": true,
  "description": "Github Action to read a secret value from AWS Secret Manager",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "dist": "npm run build && npm run package",
    "all": "npm run build && npm run format && npm run lint && npm run package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/t-botz/aws-secrets-manager-read-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "aws",
    "secrets"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "1.11.1",
    "@aws-sdk/client-secrets-manager": "3.782.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.24.0",
    "@types/node": "^20.17.28",
    "@typescript-eslint/parser": "^8.28.0",
    "@vercel/ncc": "^0.38.3",
    "eslint": "^9.23.0",
    "js-yaml": "^4.1.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.29.0"
  }
}
